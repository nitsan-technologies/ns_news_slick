{
    "name": "nitsan/ns-news-slick",
    "type": "typo3-cms-extension",
    "description": "This extension allow you to create beautiful slider for your news images on your website.",
    "homepage": "https://extensions.typo3.org/extension/ns_news_slick/",
    "license": "GPL-2.0+",
    "keywords": [
        "TYPO3 CMS",
        "News Slider",
        "NITSAN",
        "Slick Slider"
    ],
    "support": {
        "issues": "https://github.com/nitsan-technologies/ns_news_slick/issues"
    },
    "require": {
        "typo3/cms-core": ">=6.2.0",
        "georgringer/news": ">=3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Nitsan\\NsNewsSlick\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/ns-news-slick": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor/",
        "bin-dir": ".Build/bin/"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "ns_news_slick"
        }
    }
}